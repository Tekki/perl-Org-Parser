#!/usr/bin/perl

# a simple example script to dump org structure

# workaround for dzil
package dump_org_structure;

use 5.010;
use strict;
use warnings;
use Log::Any qw($log);

use String::Escape qw(elide printable);
use Org::Parser;
use Org::Dump;

my $doc = Org::Parser->new->parse([<>]);
print Org::Dump::dump_element($doc);
